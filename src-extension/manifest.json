{
    "name": "Inject Ext",
    "version": "0.0.1",
    "manifest_version": 3,
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "better-music.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ],
    "permissions": [],
    "content_scripts": [
        {
            "js": [
                "content-script.js"
            ],
            "matches": [
                "*://music.yandex.ru/*"
            ],
            "run_at": "document_end"
        }
    ]
}